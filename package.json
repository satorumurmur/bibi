{
  "name": "bibi",
  "version": "1.0.0-pr",
  "author": {
    "name": "Satoru MATSUSHIMA"
  },
  "description": "BiB/i | EPUB Reader on your website.",
  "keywords": [
    "EPUB",
    "Reading System",
    "Viewer",
    "Books-in-Browsers"
  ],
  "license": "MIT",
  "homepage": "https://bibi.epub.link",
  "repository": {
    "type": "git",
    "url": "git://github.com/satorumurmur/bibi.git"
  },
  "bugs": "https://github.com/satorumurmur/bibi/issues",
  "readmeFilename": "README.md",
  "scripts": {
    "produce": "webpack --config webpack.config.js --progress --colors false --mode production",
    "develop": "webpack --config webpack.config.js --progress --colors --mode development",
    "produce:watch": "webpack --config webpack.config.js --progress --colors --mode production --watch",
    "develop:watch": "webpack --config webpack.config.js --progress --colors --mode development --watch",
    "serve": "browser-sync --config bs-config.js",
    "build": "npm run produce",
    "start": "npm run develop:watch",
    "distribute": "npm run build && gulp distribute"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^8.6.5",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^3.1.4",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "del": "^2.2.2",
    "file-loader": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-fix-style-only-entries": "^0.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "classlist-polyfill": "^1.2.0",
    "current-script-polyfill": "^1.0.0",
    "custom-event-polyfill": "^1.0.7",
    "easing-js": "^1.1.2",
    "js-yaml": "^3.13.1",
    "jszip": "^3.2.1",
    "jszip-utils": "0.0.2",
    "material-icons": "^0.3.1",
    "native-promise-only": "^0.8.1",
    "polyfill-array-includes": "^2.0.0",
    "sml.js": "^1.0.17",
    "string.padstart": "^0.1.1",
    "text-encoding-utf-8": "^1.0.2"
  }
}
